{
  "name": "uat",
  "description": "Role definition for Play server",
  "default_attributes": {
    "wds-play": {
      "username": "ubuntu"
    }
  },
  "override_attributes": {
    "java": {
      "install_flavour": "oracle",
      "jdk_version" : "8",
      "oracle": {
        "accept_oracle_download_terms": true
      }
    }
  },
  "run_list": [
    "recipe[java]"
    "recipe[wds-play]"
  ]
}